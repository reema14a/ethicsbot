{
  "manifest_version": 3,
  "name": "EthicsBot Watchdog",
  "version": "0.1.1",
  "permissions": ["contextMenus", "activeTab", "scripting", "notifications"],
  "host_permissions": ["<all_urls>", "http://127.0.0.1:8000/*"],
  "background": { "service_worker": "background.js" },
  "action": { "default_title": "Watchdog" }
}
